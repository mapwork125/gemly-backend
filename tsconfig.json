{
  "compilerOptions": {
    "module": "CommonJS", // Change from "ESNext" to "CommonJS"
    "target": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "sourceMap": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "rootDir": "src",
    "noImplicitAny": false
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
